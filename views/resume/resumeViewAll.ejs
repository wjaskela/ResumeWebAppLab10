<html>
    <head>
        <title>Resumes</title>
    </head>
    <body>
        <p>
            <a href="/">Back to Home</a>
        </p>
        <h1>Resumes</h1>
        <a href="/resume/add">Add New Resume</a> <br />

        <br />

        <table border="1">
            <tr><th>Resume Name</th><th>Account Name</th></tr>
            <% for(var i=0; result.length > i; i++ ) { %>
            <tr>
                <td><a href="/resume/?resume_id=<%= result[i].resume_id %>"><%= result[i].resume_name %></a></td>
                <td><a href="/account/?account_id=<%= result[i].account_id %>"><%= result[i].first_name %>
                        <%= result[i].last_name %></a></td>
            </tr>
            <% } %>
        </table>
    </body>

</html>